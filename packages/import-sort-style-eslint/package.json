{
  "name": "import-sort-style-eslint",
  "version": "5.0.0",
  "description": "An import-sort style that is compatible with ESLint's sort-imports rule",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "build:watch": "tsc -p src -w",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "test:watch": "mocha -w --require ts-node/register --recursive \"test/**/*.ts\""
  },
  "author": "Renke Grunwald <renke@renke.org> (https://github.com/renke)",
  "repository": "renke/import-sort",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/lodash": "^4.14.110",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.3.5",
    "chai": "^4.0.2",
    "import-sort-style": "^5.0.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "eslint": "^5.0.0",
    "lodash": "^4.17.10"
  }
}
