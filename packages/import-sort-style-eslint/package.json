{
  "name": "import-sort-style-eslint",
  "version": "3.0.0",
  "description": "An import-sort style that is compatible with ESLint's sort-imports rule",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "watch:build": "tsc -p src -w",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive test",
    "watch:test": "mocha -w --compilers ts:ts-node/register,tsx:ts-node/register --recursive test"
  },
  "author": "Renke Grunwald <renke@renke.org> (https://github.com/renke)",
  "repository": "renke/import-sort",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/lodash": "^4.14.66",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.31",
    "chai": "^4.0.2",
    "import-sort-style": "^3.0.0",
    "mocha": "^3.4.2",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "eslint": "^4.0.0",
    "lodash": "^4.17.4"
  }
}
