    "chai": "^4.0.2",
    "import-sort-parser": "^2.0.14",
    "mocha": "^3.4.2",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "typescript": "^2.3.4"
  }
