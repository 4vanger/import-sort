{
  "name": "import-sort",
  "version": "4.2.0",
  "description": "Sort ES2015 (aka ES6) imports programmatically.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "build:watch": "tsc -p src -w",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "test:watch": "mocha -w --require ts-node/register --recursive \"test/**/*.ts\""
  },
  "author": "Renke Grunwald <renke@renke.org> (https://github.com/renke)",
  "repository": "renke/import-sort",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.3.5",
    "@types/resolve": "^0.0.8",
    "chai": "^4.0.2",
    "eslint": "^5.0.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-parser-typescript": "^4.2.0",
    "import-sort-style-eslint": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "detect-newline": "^2.1.0",
    "import-sort-parser": "^4.2.0",
    "import-sort-style": "^4.2.0",
    "is-builtin-module": "^2.0.0",
    "resolve": "^1.8.1"
  }
}
